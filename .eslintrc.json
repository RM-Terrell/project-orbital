{
    "env": {
        "browser": true,
        "es6": true
    },
    "extends": "airbnb",
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 2018,
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {
        "linebreak-style":"off",
        "prefer-const":"error",
        "arrow-body-style":"off",
        "react/prefer-stateless-function": "off",
        "react/jsx-filename-extension": "off",
        "react/jsx-curly-brace-presence": "off"
    }
}